    <dependency>
      <groupId>org.apache.poi</groupId>
      <artifactId>poi</artifactId>
      <version>${poi.version}</version>
      <scope>runtime</scope>
    </dependency>
    <!-- Override JSON-lib use of old commons-lang 2.5 -->
