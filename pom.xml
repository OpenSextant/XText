  <modelVersion>4.0.0</modelVersion>
  <groupId>org.opensextant</groupId>
  <version>2.10.7</version>
  <name>XText</name>
  <artifactId>opensextant-xponents-xtext</artifactId>
  <description>Content extraction simplified! Retrieve text, data and metadata from binary documents using Tika and similar toolkits</description>
  <inceptionYear>2013</inceptionYear>
  <url>https://opensextant.github.io/XText</url>
  <scm>
    <connection>scm:git:https://github.com/OpenSextant/XText.git</connection>
    <developerConnection>scm:git:https://github.com/OpenSextant/XText.git</developerConnection>
    <url>https://github.com/OpenSextant/XText.git</url>
  </scm>
  <licenses>
    <license>
      <name>Apache 2</name>
      <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
    </license>
  </licenses>
  <developers>
    <developer>
      <name>Marc Ubaldino</name>
      <email>ubaldino@mitre.org</email>
      <organization>MITRE</organization>
      <roles>
        <role>Lead</role>
      </roles>
    </developer>
  </developers>
